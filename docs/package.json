{
  "name": "docs-audius-org",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "start-remote": "npm run start -- --host 0.0.0.0",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "prepare:translations": "sh scripts/prepare-translations.sh",
    "link-typedoc-plugin": "ln -sf $(pwd)/typedoc-plugin-audius-theme $(pwd)/node_modules",
    "init-sdk-docgen": "cd $(pwd)/node_modules/@audius/sdk && npm i && cd ../../../ && npm run link-typedoc-plugin"
  },
  "dependencies": {
    "@audius/sdk": "file:../libs",
    "@docusaurus/core": "2.0.0-beta.20",
    "@docusaurus/preset-classic": "2.0.0-beta.20",
    "@docusaurus/theme-search-algolia": "2.0.0-beta.20",
    "@mdx-js/react": "^1.6.21",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "docusaurus-plugin-typedoc": "^0.17.5",
    "file-loader": "^6.2.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typedoc": "^0.22.17",
    "typedoc-plugin-markdown": "^3.12.1",
    "typescript": "^4.6.2",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
