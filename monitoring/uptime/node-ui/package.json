{
  "name": "node-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "env-cmd -f .env.dev.local vite",
    "stage": "env-cmd -f .env.stage.local vite",
    "prod": "env-cmd -f .env.prod.local vite",
    "stage:preview": "env-cmd -f .env.stage.local vite build && vite preview",
    "prod:preview": "env-cmd -f .env.prod.local vite build && vite preview",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@audius/sdk": "3.0.11-beta.21",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@tanstack/react-query": "5.8.4",
    "@tanstack/react-query-devtools": "5.8.4",
    "@tremor/react": "^3.11.1",
    "autoprefixer": "^10.4.16",
    "bn.js": "5.2.1",
    "ethers": "6.8.1",
    "postcss": "^8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.2.4",
    "viem": "2.0.0-beta.6",
    "wagmi": "2.0.0-beta.5",
    "web3": "1.8.2",
    "web3-utils": "1.8.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/eslint-plugin-query": "5.8.4",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitejs/plugin-react-swc": "3.3.2",
    "env-cmd": "10.1.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "prettier": "3.1.0",
    "prettier-config-standard": "7.0.0",
    "tailwindcss": "^3.3.5",
    "typescript": "5.2.2",
    "vite": "4.4.5",
    "vite-plugin-node-polyfills": "0.16.0"
  }
}
